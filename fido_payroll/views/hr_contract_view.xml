<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record model="ir.ui.view" id="fido_hr_contract_view_form">
            <field name="name">hr.contract.ext.form</field>
            <field name="model">hr.contract</field>
            <field name="inherit_id" ref="hr_contract.hr_contract_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='wage']" position="after">
                    <field name="bagged_mult" />
                </xpath>
                                
                <field name="bagged_mult" position="after">
                    <field name="cratesold_mult"/>
                </field>
                <field name="cratesold_mult" position="after">
                    <field name="dispsold_mult"/>
                </field>
                <field name="dispsold_mult" position="after">
                    <field name="kpbgsold_mult"/>
                </field>
                <field name="kpbgsold_mult" position="after">
                    <field name="obbgsold_mult"/>
                </field>
                
                
                <field name="obbgsold_mult" position="after">
                    <field name="sal_adv"/>
                </field>
                <field name="sal_adv" position="after">
                    <field name="loan_adv"/>
                </field>
                <field name="loan_adv" position="after">
                    <field name="payee"/>
                </field>
                <field name="payee" position="after">
                    <field name="days_absent"/>
                </field>
                <field name="days_absent" position="after">
                    <field name="kpbg_sold"/>
                </field>
                <field name="kpbg_sold" position="after">
                    <field name="obbg_sold"/>
                </field>
                <field name="obbg_sold" position="after">
                    <field name="disp_sold"/>
                </field>
                <field name="disp_sold" position="after">
                    <field name="crate_sold"/>
                </field>
                
            </field>
        </record>

    </data>
</openerp>